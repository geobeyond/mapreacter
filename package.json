		"extract-text-webpack-plugin": "^3.0.1",
		"file-loader": "^1.1.5",
		"html-webpack-plugin": "^2.30.1",
		"jest": "^20.0.4",
		"json-loader": "^0.5.7",
		"less": "^3.0.0-alpha.3",
		"less-loader": "^4.0.5",
		"minimist": "^1.2.0",
		"node-sass": "^4.5.3",
		"npm-run-all": "^4.1.1",
		"ol": "^4.3.1",
		"react-test-renderer": "^16.0.0",
		"replacestream": "^4.0.3",
		"sass-loader": "^6.0.6",
		"sdk-tools": "^1.1.0",
		"style-loader": "^0.19.0",
		"uglifyjs-webpack-plugin": "^1.0.0-beta.3",
		"webpack": "^3.7.1",
		"webpack-dev-server": "^2.9.1"
