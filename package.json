	"main": "index.js",
	"repository": {},
	"scripts": {
		"test": "jest",
		"createdir": "node createbuild.js",
		"test:watch": "jest --watch",
		"start:dev": "NODE_ENV=development node_modules/.bin/webpack-dev-server --progress --colors --hot --inline",
		"start": "node_modules/.bin/npm-run-all createdir start:dev",
		"build": "NODE_ENV=development node_modules/.bin/webpack",
		"build:release": "NODE_ENV=production node_modules/.bin/webpack --context=mapreacter",
		"build:clean": "rm -rf dist",
		"createzip": "node createpackage.js -- --output-file=mapreacter.zip",
		"package": "node_modules/.bin/npm-run-all build:clean build:release createzip"
	},
	"dependencies": {
		"@boundlessgeo/sdk": "2.1.0-beta2",
		"material-ui": "^0.19.4",
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-redux": "^5.0.6",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"redux": "^3.7.2",
		"redux-thunk": "^2.2.0"
	},
	"devDependencies": {
		"archiver": "^2.1.0",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-env": "^1.6.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"canvas": "^1.6.7",
		"css-loader": "^0.28.7",
